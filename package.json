{
  "name": "heady-ecosystem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "prepare": "husky install",
    "setup:desktop": "node scripts/setup-desktop-icons.js",
    "merge": "node scripts/auto-merge.js",
    "merge:verbose": "node scripts/auto-merge.js --verbose",
    "merge:triple": "node scripts/auto-merge.js --triple --verbose"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "tsx": "^4.7.0",
    "turbo": "^1.10.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
